{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WorkerOptions": {
    "SleepMilliseconds": 300,
    "Routines": {
      "TestRoutineKey": {
        "Concurrency": 1,
        "Schedule": "* * * * *",
        "Timeout": 1000
      },
      "TestRoutineKey2": {
        "Concurrency": 1,
        "Schedule": "*/2 * * * *",
        "Timeout": 1000
      },
      "TestRoutineKeyInvalid": {
        "Concurrency": 1,
        "Schedule": "61 * * * *",
        "Timeout": 1000
      }
    }
  }
}
